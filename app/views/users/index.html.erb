  <div>
    <form action="<%= dashboard_path %>">
      <input type="hidden" name="log_out" value="true">
      <input type="submit" value="Log Out">
    </form>
  </div>
  <hr>

<h2>List of Users</h2>


  <table>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
    <% @users.each do |u| %>
    <tr>
      <td><%= u.id %></td>
      <td><a href='<%= user_path(u.id) %>'><%= u.email %></a></td>
      <td><a href= "<%= edit_profile_path %>">Edit a User</a></td>
      <td><a href= "<%= user_path(u.id) %>" data-method='delete' data-confirm="Are you sure?">Delete a User</a></td>
    </tr>
    <% end %>
  </table>

  <a href="<%= new_user_path %>">Add New User</a>
